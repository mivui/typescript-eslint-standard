{
  "name": "typescript-eslint-standard",
  "version": "9.58.0",
  "description": "quickly start eslint in typescript",
  "type": "module",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "scripts": {
    "eslint": "eslint --fix",
    "build": "rollup -c rollup.config.js",
    "test": "vitest"
  },
  "dependencies": {
    "@vitest/eslint-plugin": "^1.3.4",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.38.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.1.0",
    "rollup": "^4.45.3",
    "rollup-plugin-dts": "^6.2.1",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "author": "mivui",
  "license": "MIT",
  "keywords": [
    "eslint",
    "prettier",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mivui/typescript-eslint-standard.git"
  },
  "bugs": {
    "url": "https://github.com/mivui/typescript-eslint-standard/issues"
  },
  "homepage": "https://github.com/mivui/typescript-eslint-standard#readme"
}
