{
  "name": "typescript-eslint-standard",
  "version": "9.17.0",
  "description": "quickly start eslint in typescript",
  "type": "module",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "scripts": {
    "eslint": "eslint --fix",
    "build": "rollup -c rollup.config.js",
    "test": "vitest"
  },
  "dependencies": {
    "@vitest/eslint-plugin": "^1.1.7",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.3.3",
    "typescript-eslint": "8.12.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/node": "^22.8.6",
    "rollup": "^4.24.3",
    "rollup-plugin-dts": "^6.1.1",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "author": "mivui",
  "license": "MIT",
  "keywords": [
    "eslint",
    "prettier",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mivui/typescript-eslint-standard.git"
  },
  "bugs": {
    "url": "https://github.com/mivui/typescript-eslint-standard/issues"
  },
  "homepage": "https://github.com/mivui/typescript-eslint-standard#readme"
}
