{
  "name": "typescript-eslint-standard",
  "version": "9.12.0",
  "description": "quickly start eslint in typescript",
  "type": "module",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "scripts": {
    "eslint": "eslint --fix",
    "build": "rollup -c rollup.config.js",
    "test": "vitest"
  },
  "dependencies": {
    "@vitest/eslint-plugin": "^1.1.6",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.3.3",
    "typescript-eslint": "8.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.0",
    "@types/node": "^22.7.4",
    "rollup": "^4.24.0",
    "rollup-plugin-dts": "^6.1.1",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "vitest": "^2.1.2"
  },
  "author": "mivui",
  "license": "MIT",
  "keywords": [
    "eslint",
    "prettier",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mivui/typescript-eslint-standard.git"
  },
  "bugs": {
    "url": "https://github.com/mivui/typescript-eslint-standard/issues"
  },
  "homepage": "https://github.com/mivui/typescript-eslint-standard#readme"
}
